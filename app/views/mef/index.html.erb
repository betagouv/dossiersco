<p id="notice"><%= notice %></p>

<h1>Mef</h1>

<table>
  <thead>
    <tr>
      <th>Libelle</th>
      <th>Code</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @mef.each do |mef| %>
      <tr>
        <td><%= mef.libelle %></td>
        <td><%= mef.code %></td>
        <td><%= link_to 'Editer', edit_mef_path(mef) %></td>
        <td><%= link_to 'Supprimer', mef, method: :delete, data: { confirm: 'Etes vous sure ?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nouveau Mef', new_mef_path %>
